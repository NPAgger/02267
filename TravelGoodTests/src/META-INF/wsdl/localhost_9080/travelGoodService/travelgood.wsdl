                <xsd:sequence>
                    <xsd:element ref="tns:Message"/>
                    <xsd:element ref="tns:flightBookingArray"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:element name="BookItineraryFault" type="tns:bookItineraryFaultType"/>
